      <pou name="CounterIL" pouType="functionBlock">
        <interface>
          <localVars>
            <variable name="Cnt">
              <type>
                <INT/>
              </type>
            </variable>
          </localVars>
          <inputVars>
            <variable name="Reset">
              <type>
                <BOOL/>
              </type>
            </variable>
          </inputVars>
          <outputVars>
            <variable name="OUT">
              <type>
                <INT/>
              </type>
            </variable>
          </outputVars>
          <externalVars constant="true">
            <variable name="ResetCounterValue">
              <type>
                <INT/>
              </type>
            </variable>
          </externalVars>
        </interface>
        <body>
          <IL>
            <xhtml:p><![CDATA[LD Reset
JMPC ResetCnt

(* increment counter *)
LD Cnt
ADD 1
JMP QuitFb

ResetCnt:
(* reset counter *)
LD ResetCounterValue

QuitFb:
(* save results *)
